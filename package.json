{
  "name": "openfin-automation",
  "version": "1.0.0",
  "description": "Automate OpenFin application with WebDriverIO using DevTools protocol",
  "main": "index.js",
  "scripts": {
    "test": "CHROMEDRIVER_SKIP_DOWNLOAD=true WDIO_DISABLE_CHROMEDRIVER=true wdio run wdio.conf.js",
    "test:login": "wdio run wdio.conf.js --spec ./features/login.feature",
    "test:historical-blotter": "wdio run wdio.conf.js --spec ./features/historical-blotter.feature"
  },
  "devDependencies": {
    "@wdio/cli": "^8.16.18",
    "@wdio/cucumber-framework": "^8.16.18",
    "@wdio/devtools-service": "^8.42.0",
    "@wdio/local-runner": "^8.16.18",
    "@wdio/spec-reporter": "^8.16.18",
    "webdriverio": "^8.16.18"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "devtools": "^8.42.0",
    "fs-extra": "^11.3.0"
  }
}
